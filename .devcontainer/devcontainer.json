{
    "image": "mcr.microsoft.com/devcontainers/rust:1-1-bullseye",
    "extensions": [
        "rust-lang.rust-analyzer"
    ],
    "settings": {
        "rust-analyzer.checkOnSave": true,
        "editor.formatOnSave": true
    },
    "runArgs": [
        "--privileged"
    ],
    "remoteUser": "root"
}